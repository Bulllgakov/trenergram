# Telegram Bot
BOT_TOKEN=your_bot_token_here
BOT_USERNAME=TrenergramBot

# Database
DATABASE_URL=postgresql+asyncpg://user:password@localhost/trenergram
# For development use SQLite:
# DATABASE_URL=sqlite+aiosqlite:///./trenergram.db

# Redis
REDIS_URL=redis://localhost:6379/0

# Security
SECRET_KEY=your-secret-key-here-generate-with-openssl-rand-hex-32
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=10080

# Environment
ENVIRONMENT=development
DEBUG=True

# Domain
DOMAIN=trenergram.ru
WEBAPP_URL=https://trenergram.ru

# Admin credentials
SUPER_ADMIN_EMAIL=admin@trenergram.ru
SUPER_ADMIN_PASSWORD=changeme

# Payments (for future)
PAYMENT_PROVIDER_TOKEN=test_payment_token

# Celery
CELERY_BROKER_URL=redis://localhost:6379/1
CELERY_RESULT_BACKEND=redis://localhost:6379/2